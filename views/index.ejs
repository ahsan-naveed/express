<ul class="quotes">
  <% for(var i=0; i<quotes.length; i++) {%>
    <li class="quote">
      <span>
        <%= quotes[i].name %>
      </span>
      <span>
        <%= quotes[i].quote %>
      </span>
    </li>
    <% } %>
</ul>

<form action="/quotes" method="POST">
  <fieldset>
    <h3>Enter quote please: </h3>
    <input type="text" name="quote" placeholder="quote">
    <input type="text" name="name" placeholder="name">
    <input type="submit" value="Submit">
  </fieldset>
</form>